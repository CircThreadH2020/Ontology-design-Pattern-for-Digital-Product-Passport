        <untitled-ontology-135:semanticId>https://admin-shell.io/idta/HierarchicalStructures/BulkCount/1/0</untitled-ontology-135:semanticId>
        <rdfs:comment xml:lang="en">To be used if bulk components are referenced, e.g., a 10x M4x30 screw.
Additional constraint: With bulk count only a reference to an asset with kind type is allowed, e.g., the M4x30 type asset.</rdfs:comment>
        <rdfs:label xml:lang="en">BulkCount</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/monirehpourjafarian/ontologies/2024/8/untitled-ontology-135#EntryNode -->

    <owl:Class rdf:about="http://www.semanticweb.org/monirehpourjafarian/ontologies/2024/8/untitled-ontology-135#EntryNode">
