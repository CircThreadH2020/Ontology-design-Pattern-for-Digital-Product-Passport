    <owl:ObjectProperty rdf:about="https://w3id.org/dot#aggregatesDamageElement">
        <rdfs:domain rdf:resource="https://w3id.org/dot#DamageArea"/>
        <rdfs:range rdf:resource="https://w3id.org/dot#DamageElement"/>
